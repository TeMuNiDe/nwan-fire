{
  "name": "nwan-fire-server",
  "main": "./server/app.js",
  "description": "A basic web app to track and analysis personal networth for fire",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "debug_react": "react-scripts start",
    "build_app": "react-scripts build",
    "build_server": "tsc",
    "start_server": "node --trace-warnings  -r dotenv/config ./server/app.js",
    "debug_server": "tsc && node -r dotenv/config ./server/app.js",
    "refresh_config": "sed -i \"3 s?esnext?commonjs?\" tsconfig.json && sed -i \"16,24 d\" tsconfig.json && sed -i \"15 s?,??\" tsconfig.json && rm -f .\\src\\react-app-env.d.ts"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "16.10.2",
    "react-scripts": "^4.0.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@babel/core": "^7.13.0",
    "@ibm-cloud/cloudant": "0.0.20",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@types/cfenv": "^1.2.1",
    "@types/node": "^16.10.2",
    "body-parser": "^1.19.0",
    "cfenv": "^1.2.4",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "mongodb": "^3.6.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recharts": "^2.0.10",
    "tough-cookie": "^4.0.0",
    "watchpack": "^2.2.0"
  },
  "author": "Vihaari Varma",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
