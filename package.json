{
  "name": "nwan-fire-server",
  "main": "./server/app.js",
  "description": "A basic web app to track and analysis personal networth for fire",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "debug_react": "react-scripts start",
    "build_app": "react-scripts build",
    "build_server": "tsc",
    "start": "node ./server/app.js",
    "debug_server": "tsc && node ./server/app.js",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "react-scripts": "^1.1.5",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@cloudant/cloudant": "^4.3.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "body-parser": "^1.19.0",
    "cfenv": "^1.2.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recharts": "^2.0.10"
  },
  "author": "Vihaari Varma",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
